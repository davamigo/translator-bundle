{
    "name": "davamigo/translator-bundle",
    "type": "symfony-bundle",
    "description": "A Symfony2 bundle to help you to translate your Symfony2 applications.",
    "keywords": [ "translator", "symfony2" ],
    "homepage": "https://github.com/davamigo",
    "license": "MIT",
    "authors": [
        {
            "name": "David Amigo",
            "email": "davamigo@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/framework-bundle": "~2.3|~3.0",
        "symfony/browser-kit": "~2.3|~3.0",
        "symfony/finder": "~2.3|~3.0",
        "symfony/form": "~2.3|~3.0",
        "symfony/validator": "~2.3|~3.0",
        "sensio/framework-extra-bundle": "~3.0,>=3.0.2",
        "liuggio/ExcelBundle": "^2.0",
        "phpoffice/phpexcel": "^1.8"
    },
    "require-dev": {
        "phpunit/phpunit": "~4"
    },
    "autoload": {
        "psr-4": {
            "Davamigo\\TranslatorBundle\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
